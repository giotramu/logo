{
  "name": "@giotramu/logo",
  "version": "2.2.2",
  "description": "Web Component version of personal logo and related resources.",
  "author": "giotramu <https://github.com/giotramu>",
  "homepage": "https://github.com/giotramu/logo#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/logo.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/logo/issues"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/webc/esm/webc.js",
  "types": "lib/webc/types/index.d.ts",
  "files": [
    "lib/favicon",
    "lib/loader",
    "lib/svg",
    "lib/webc"
  ],
  "prettier": "@giotramu/prettier-config",
  "scripts": {
    "check:tsc": "tsc --noEmit",
    "check:css": "stylelint ./src/**/*.css",
    "check:src": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check": "run-p -cln check:*",
    "test": "stencil test --spec --coverage",
    "dev": "stencil build --dev --watch --serve",
    "clean": "del ./coverage ./lib/webc ./lib/loader ./www",
    "prebuild": "npm run clean",
    "build": "stencil build"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.2.0",
    "@giotramu/prettier-config": "^1.4.0",
    "@giotramu/stylelint-config": "^12.0.1",
    "@types/jest": "^27.5.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.34.0",
    "eslint-config-contactlab": "^9.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.0",
    "jest": "^27.5.0",
    "jest-cli": "^27.5.0",
    "npm-run-all": "^4.1.0",
    "prettier": "^2.8.0",
    "stylelint": "^15.1.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@stencil/core": "3.1.0"
  },
  "keywords": [
    "custom-elements",
    "logo",
    "stencil-components",
    "stenciljs",
    "web-components"
  ]
}

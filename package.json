{
  "name": "@giotramu/logo",
  "version": "1.8.0",
  "description": "Web Component version of personal logo and related resources.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/logo#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/logo.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/logo/issues"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "unpkg": "dist/giotramu-logo/giotramu-logo.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/",
    "resources/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:src": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint:css & npm run lint:src",
    "test": "stencil test --spec --coverage",
    "test:e2e": "stencil test --e2e",
    "dev": "npm run build -- --dev --watch --serve",
    "clean": "del ./coverage ./dist ./loader ./www",
    "prebuild": "npm run clean",
    "build": "stencil build"
  },
  "prettier": "@giotramu/prettier-config",
  "devDependencies": {
    "@arkweid/lefthook": "0.7.6",
    "@giotramu/prettier-config": "1.1.2",
    "@giotramu/stylelint-config": "7.0.2",
    "@stencil/core": "2.5.2",
    "@types/jest": "26.0.23",
    "@types/puppeteer": "5.4.3",
    "del-cli": "3.0.1",
    "eslint": "7.29.0",
    "eslint-config-contactlab": "6.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.24.0",
    "jest": "27.0.4",
    "jest-cli": "26.6.3",
    "prettier": "2.3.2",
    "puppeteer": "6.0.0",
    "stylelint": "13.13.1",
    "typescript": "4.3.5"
  },
  "keywords": [
    "custom-elements-v1",
    "logo",
    "stencil-components",
    "stenciljs",
    "web-components"
  ]
}

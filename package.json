{
  "name": "@giotramu/logo",
  "version": "1.6.0",
  "description": "Web Component version of personal logo and related resources.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/logo#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/logo.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/logo/issues"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "types": "./dist/types/components.d.ts",
  "unpkg": "./dist/giotramu-logo/giotramu-logo.js",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "files": [
    "dist/",
    "loader/",
    "resources/"
  ],
  "scripts": {
    "check:src": "tsc --noEmit",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:src": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint:css & npm run lint:src",
    "pretest": "npm run check:src & npm run lint",
    "test": "stencil test --spec --e2e --coverage",
    "dev": "npm run build -- --dev --watch --serve",
    "clean": "rimraf ./coverage ./dist ./loader ./www",
    "prebuild": "npm run clean",
    "build": "stencil build"
  },
  "devDependencies": {
    "@giotramu/stylelint-config": "5.1.0",
    "@stencil/core": "2.0.3",
    "@types/jest": "26.0.14",
    "@types/puppeteer": "3.0.2",
    "eslint": "7.12.0",
    "eslint-config-contactlab": "5.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "jest-cli": "26.6.1",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "puppeteer": "5.4.1",
    "rimraf": "3.0.2",
    "stylelint": "13.7.2",
    "typescript": "4.0.5"
  },
  "keywords": [
    "custom-elements-v1",
    "logo",
    "stencil-components",
    "stenciljs",
    "web-components"
  ]
}

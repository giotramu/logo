{
  "name": "@giotramu/logo",
  "version": "2.1.1",
  "description": "Web Component version of personal logo and related resources.",
  "author": "giotramu <https://github.com/giotramu>",
  "homepage": "https://github.com/giotramu/logo#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/logo.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/logo/issues"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/webc/cjs/webc.cjs.js",
  "module": "lib/webc/esm/webc.js",
  "types": "lib/webc/types/index.d.ts",
  "unpkg": "lib/webc/esm/webc.js",
  "collection": "lib/webc/collection/collection-manifest.json",
  "collection:main": "lib/webc/collection/index.js",
  "files": [
    "lib/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "check:tsc": "tsc --noEmit",
    "check:css": "stylelint ./src/**/*.css",
    "check:src": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check": "run-p -cln check:*",
    "test": "stencil test --spec --coverage",
    "dev": "stencil build --dev --watch --serve",
    "clean": "del ./coverage ./lib/webc ./lib/loader ./www",
    "prebuild": "npm run clean",
    "build": "stencil build"
  },
  "prettier": "@giotramu/prettier-config",
  "dependencies": {
    "@stencil/core": "2.19.3"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.2.1",
    "@giotramu/prettier-config": "^1.4.0",
    "@giotramu/stylelint-config": "^11.1.1",
    "@types/jest": "^27.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-contactlab": "^9.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.15.0",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "custom-elements",
    "logo",
    "stencil-components",
    "stenciljs",
    "web-components"
  ]
}

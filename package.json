{
  "name": "@giotramu/logo",
  "version": "1.4.1",
  "description": "Web Component version of my logo and related resources.",
  "author": "giotramu <https://twitter.com/giotramu>",
  "homepage": "https://github.com/giotramu/logo#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/logo.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/logo/issues"
  },
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "unpkg": "dist/giotramu-logo/giotramu-logo.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/",
    "resources/"
  ],
  "scripts": {
    "clean": "rimraf dist loader www",
    "check:src": "tsc --noEmit",
    "lint:css": "stylelint ./src/components/**/*.css",
    "lint:src": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "lint": "npm run lint:css && npm run lint:src",
    "test": "stencil test --spec --e2e --coverage",
    "dev": "npm run build -- --dev --watch --serve",
    "prebuild": "npm run clean",
    "build": "stencil build"
  },
  "devDependencies": {
    "@giotramu/stylelint-config": "3.2.0",
    "@stencil/core": "1.14.0",
    "@types/jest": "25.2.3",
    "@types/puppeteer": "3.0.0",
    "eslint": "7.2.0",
    "eslint-config-contactlab": "5.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "puppeteer": "3.2.0",
    "rimraf": "3.0.2",
    "stylelint": "13.6.0"
  },
  "keywords": [
    "custom-elements-v1",
    "logo",
    "stencil-components",
    "stenciljs",
    "web-components"
  ]
}

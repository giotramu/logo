{
  "name": "@giotramu/logo",
  "version": "2.0.0-rc.4",
  "description": "Web Component version of personal logo and related resources.",
  "author": "giotramu <https://github.com/giotramu>",
  "homepage": "https://github.com/giotramu/logo#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giotramu/logo.git"
  },
  "bugs": {
    "url": "https://github.com/giotramu/logo/issues"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/giotramu-logo/giotramu-logo.esm.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/",
    "assets/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "check:tsc": "tsc --noEmit",
    "check:css": "stylelint ./src/**/*.css",
    "check:src": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check": "run-p -cln check:*",
    "test": "stencil test --spec --coverage",
    "dev": "stencil build --dev --watch --serve",
    "clean": "del ./coverage ./dist ./loader ./www",
    "prebuild": "npm run clean",
    "build": "stencil build"
  },
  "prettier": "@giotramu/prettier-config",
  "dependencies": {
    "@stencil/core": "2.18.1"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "1.1.3",
    "@giotramu/prettier-config": "1.4.0",
    "@giotramu/stylelint-config": "11.1.0",
    "@types/jest": "27.0.3",
    "del-cli": "5.0.0",
    "eslint": "8.26.0",
    "eslint-config-contactlab": "9.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.10",
    "jest": "27.4.5",
    "jest-cli": "27.4.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "stylelint": "14.14.0",
    "typescript": "4.8.4"
  },
  "keywords": [
    "custom-elements",
    "logo",
    "stencil-components",
    "stenciljs",
    "web-components"
  ]
}

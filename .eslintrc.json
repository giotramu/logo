{
  "extends": [
    "@giotramu/eslint-config",
    "@giotramu/eslint-config/typescript",
    "@giotramu/eslint-config/react"
  ],

  "settings": {
    "react": {
      "pragma": "h"
    }
  },

  "rules": {
    "react/no-unknown-property": [
      "error",
      {
        "ignore": ["class", "clip-rule", "fill-rule"]
      }
    ]
  }
}

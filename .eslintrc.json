{
  "extends": [
    "@detra-lab/eslint-config",
    "@detra-lab/eslint-config/typescript",
    "@detra-lab/eslint-config/react"
  ],
  "parserOptions": {
    "project": ["./tsconfig.test.json"]
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "rules": {
    "react/no-unknown-property": [
      "error",
      {
        "ignore": ["class", "clip-rule", "fill-rule"]
      }
    ]
  }
}
